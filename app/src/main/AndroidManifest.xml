<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.proyoJon">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/lgo2"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.proyoJon.EmergencyService"></activity>
        <activity android:name="com.example.proyoJon.VerifyActivityService" />
        <activity android:name="com.example.proyoJon.PhoneAcitivityService" />
        <activity android:name="com.example.proyoJon.ShowKormiActivity" />
        <activity android:name="com.example.proyoJon.HomeActivity" />
        <activity
            android:name="com.example.proyoJon.ShoProfileAcivity"
            android:theme="@style/Theme.AppCompat.Light.DarkActionBar" />
        <activity android:name="com.example.proyoJon.SignupActivity" />
        <activity android:name="com.example.proyoJon.VerifyActivityWorker" />
        <activity android:name="com.example.proyoJon.PhoneActivityWorker" />
        <activity android:name="com.example.proyoJon.MainActivity" />
        <activity android:name="com.example.proyoJon.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>